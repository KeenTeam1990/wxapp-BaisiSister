.container{
  padding: 0;
  height: 1500rpx;

}
.container .feed-item{
 width: 690rpx;
 padding: 30rpx 30rpx 20rpx;
 margin: 7rpx 0 6rpx 0;
 border-top: 1px solid #eee;
}
.container .feed-item .feed-source{
 width: 690rpx;
 left: 0;
 height: 50rpx;
}
.container .feed-item .feed-source .avatar{
 position: relative;
 display: inline-block;
}
.container .feed-item .feed-source .avatar image{
 display:  inline-block;
 width: 45rpx;
 height: 45rpx;
 border-radius: 45rpx;
 top: 10rpx;
 vertical-align: middle;
}
.container .feed-item .feed-source text{
 display: inline-block;
 height: 40rpx ;
 font-size: 26rpx;
 vertical-align:  middle;
 color: #a0acac;
 margin: 0 0 0 15rpx;
 line-height: 40rpx;
}
.container .feed-item .feed-source .item-more{
 display: block;
 width: 280rpx;
 height: 45rpx;
 float: right;
}
.container .feed-item .feed-content{
 padding: 10rpx 0 0 0;
}
.container .feed-item .feed-content .question
{
  text-align: center;
}